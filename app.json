{
  "expo": {
    "name": "exceed",
    "slug": "exceed",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./app/assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./app/assets/background.png",
      "resizeMode": "cover",
      "backgroundColor": "#373598"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "club.exceedtoastmasters"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/icon.png",
        "backgroundColor": "#E2E1E1"
      },
      "edgeToEdgeEnabled": true,
      "package": "club.exceedtoastmasters"
    },
    "web": {
      "favicon": "./app/assets/favicon.png"
    },
    "plugins": ["expo-secure-store"]
  }
}
